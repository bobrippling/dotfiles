! Custom ------------------------------------------------------------------
! fixes some annoying shizdongles
*backarrowKeyIsErase: true
!false


! Xft settings -------------------------------------------------------------
! DO NOT TOUCH
!Xft*hinting:        1
!Xft*dpi:            96
Xft*antialias:      true
!Xft*hintstyle:      hintslight
!Xft*rgba:           rgb

!Xft:Profont:        pixelsize=11
!Xcursor.theme:      entis
!Xcursor.theme:      entis mini
!Xcursor.theme:      GT3
!Xcursor.size:       16
!32, 48 or 64 are probably good values (16?)


! urxvt settings -----------------------------------------------------------
!urxvt*font:            xft:ProggyTinyTT:pixelsize=16
urxvt*font:            xft:profont:pixelsize=11
!urxvt*font:            -*-profont-*-*-*-*-11-*-*-*-*-*-*-*
!urxvt*font:            -*-fixed-medium-r-*-*-12-*-*-*-*-*-*-*
!urxvt*font:            -*-proggytinytt-*-*-*-*-13-*-*-*-*-*-*-*
!urxvt*font:            -misc-fixed-medium-r-*-*-10-*-*-*-*-*-*-1
!urxvt*boldfont:        xft:profont:pixelsize=10
!urxvt*boldFont:        xft:fixed:pixelsize=16:bold
!urxvt*boldFont:        -*-courier-bold-r-*-*-10-100-*-*-m-*-iso10646-1,-*-lucidatypewriter-bold-r-*-*-10-100-*-*-m-*-iso10646-1
!urxvt*italicFont:      xft:ProggyTinyTT :pixelsize=16 :style=italic
urxvt*boldFont:         -*-courier-bold-r-*-*-10-100-*-*-m-*-iso10646-1,-*-lucidatypewriter-bold-r-*-*-10-100-*-*-m-*-iso10646-1
! ----------
urxvt*title:            rxvt-unicode
urxvt*termname:         rxvt-unicode
urxvt*cursorBlink:      false
urxvt*geometry:         80x24
!urxvt*fading:           25%
urxvt*shading:          5
urxvt*tintColor:        #000000
urxvt*transparent:      true
urxvt*matcher.button:   1
!urxvt*background:       #000000
!urxvt*foreground:       grey
!urxvt*depth:            32
urxvt*scrollBar:        false
urxvt*scrollBar_right:  true
!urxvt*borderLess:       true <---------- fails hard
urxvt*inheritPixmap:    true
urxvt.tintColor:        white
urxvt.internalBorder:   2
urxvt.cursorBlink:      false
urxvt.cursorColor:      #f5a400
urxvt.borderless:       false
urxvt*cutchars:         "()*,<>[]{}|'
!                         ^------------------ delimiters for double-click mouse selection
! 10 = opaque, 90 = transparent
urxvt*shading:          20
! Colours are set at the bottom, globally
!! BEGIN urlLauncher settings !!
urxvt*perl-lib:         /usr/lib/urxvt/perl/
urxvt*perl-ext-common:  matcher
!urxvt*urlLauncher:      /opt/swiftweasel/swiftweasel
urxvt*urlLauncher:      /home/rob/bin/openurl.sh
urxvt*matcher.button:   1
!! END urlLauncher settings !!

! xterm settings -----------------------------------------------------------
xterm*background:          black
!xterm*foreground:          grey
!xterm*highlightColor:      blue
xterm*scrollBar:           false
xterm*vt100.scrollBar:     false
!xterm*vt100.font:         dejavu sans mono
!xterm*font:                dejavu sans mono
xterm*multiScroll:         on
xterm*jumpScroll:          on
xterm*SaveLines:           1000
!xterm*faceName:            terminus
!xterm*pixelsize:           ???
!xterm*faceName             pixelsize:           31
xterm*dynamicColors:       true
xterm*utf8:                2
xterm*utf8Title:           1
!xterm*eightBitInput:      true
xterm*scrollTtyKeypress:   true
!xterm*scrollTtyOutput:    false
xterm*rightScrollBar:      false
xterm*loginShell:          true
xterm*toolBar:             false

! xpdf -----------------------------------------------------------------------
xpdf*enableFreetype:      yes
xpdf*antialias:           yes
xpdf*foreground:          #000000
xpdf*background:          #999999
xpdf*urlCommand:          /home/rob/bin/openurl.sh %s
xpdf*viKeys:              on

