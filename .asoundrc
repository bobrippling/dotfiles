ctl.equal {
	type equal;
}

pcm.plugequal {
	type equal;
# Modify the line below if you don't
# want to use sound card 0.
	slave.pcm "plug:dmix";
}

pcm.equal {
	type plug;
	slave.pcm "plug:dmix";
}

pcm.dmixout {
	# for skype
	type plug
	slace { pcm "dmux" };
}

# pcm.equal {
# Or if you want the equalizer to be your
# default soundcard uncomment the following
# line and comment the above line.
pcm.!default {
	type plug;
	slave.pcm plugequal;
}
